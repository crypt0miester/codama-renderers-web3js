{
    "name": "codama-renderers-web3js",
    "version": "1.0.2",
    "description": "JavaScript renderer compatible with the solana/web3.js framework",
    "exports": {
        "types": "./dist/types/index.d.ts",
        "node": {
            "import": "./dist/index.node.mjs",
            "require": "./dist/index.node.cjs"
        }
    },
    "main": "./dist/index.node.cjs",
    "module": "./dist/index.node.mjs",
    "types": "./dist/types/index.d.ts",
    "type": "commonjs",
    "files": [
        "./dist/templates",
        "./dist/types",
        "./dist/index.*"
    ],
    "sideEffects": false,
    "keywords": [
        "solana",
        "framework",
        "standard",
        "renderers",
        "js",
        "client"
    ],
    "scripts": {
        "build": "rimraf dist && tsup && tsc -p ./tsconfig.declarations.json",
        "lint": "eslint . && prettier --check .",
        "lint:fix": "eslint --fix . && prettier --write ."
    },
    "dependencies": {
        "@codama/errors": "^1.3.7",
        "@codama/nodes": "^1.3.7",
        "@codama/renderers-core": "^1.2.2",
        "@codama/validators": "^1.3.7",
        "@codama/visitors-core": "^1.3.7",
        "@solana/codecs-strings": "^3.0.3",
        "nunjucks": "^3.2.4",
        "prettier": "^3.6.2"
    },
    "devDependencies": {
        "@changesets/changelog-github": "^0.5.1",
        "@changesets/cli": "^2.29.7",
        "@solana/eslint-config-solana": "^5.0.0",
        "@solana/prettier-config-solana": "0.0.5",
        "@types/node": "^24",
        "@types/nunjucks": "^3.2.6",
        "agadoo": "^3.0.0",
        "eslint": "^9.35.0",
        "happy-dom": "^18.0.1",
        "prettier": "^3.6.2",
        "rimraf": "6.0.1",
        "tsup": "^8.5.0",
        "typescript": "^5.9.2"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/crypt0miester/codama-renderers-web3js"
    },
    "browserslist": [
        "supports bigint and not dead",
        "maintained node versions"
    ],
    "engines": {
        "node": ">=20.18.0"
    },
    "prettier": "@solana/prettier-config-solana"
}
