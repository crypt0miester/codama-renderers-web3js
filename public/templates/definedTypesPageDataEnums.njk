{% if isDataEnum %}
  {% set strictName = definedType.name | pascalCase %}
  {% set looseName = definedType.name | pascalCase + 'Args' %}
  // Data Enum Helpers.
  {% for variant in definedType.type.variants %}
    {% set variantName = variant.name | pascalCase %}
    {% if variant.kind === 'enumStructVariantTypeNode' %}
      {% if variant.struct.fields.length > 0 %}
      export function {{ definedType.name | camelCase }}{{ variantName }}(data: Extract<{{ strictName }}, { {{ variantName }}: any }>['{{ variantName }}']): {{ strictName }} {
        return { {{ variantName }}: data } as {{ strictName }};
      }
      {% else %}
      export function {{ definedType.name | camelCase }}{{ variantName }}(): {{ strictName }} {
        return { {{ variantName }}: {} };
      }
      {% endif %}
    {% elif variant.kind === 'enumTupleVariantTypeNode' %}
      export function {{ definedType.name | camelCase }}{{ variantName }}(fields: Extract<{{ strictName }}, { {{ variantName }}: any }>['{{ variantName }}']['fields']): {{ strictName }} {
        return { {{ variantName }}: { fields } } as {{ strictName }};
      }
    {% else %}
      export function {{ definedType.name | camelCase }}{{ variantName }}(): {{ strictName }} {
        return { {{ variantName }}: {} };
      }
    {% endif %}
  {% endfor %}
{% endif %}
